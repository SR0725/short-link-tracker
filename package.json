{
  "name": "short-link-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "npx prisma generate && npm run update-geoip-conditional && next build",
    "build:turbo": "npx prisma generate && npm run update-geoip-conditional && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "update-geoip": "cd node_modules/geoip-lite && npm run-script updatedb license_key=$MAXMIND_LICENSE_KEY",
    "update-geoip-conditional": "if [ -n \"$MAXMIND_LICENSE_KEY\" ]; then npm run update-geoip; else echo \"MAXMIND_LICENSE_KEY not set, skipping GeoIP database update\"; fi"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.1.0",
    "dompurify": "^3.3.0",
    "file-type": "^21.0.0",
    "framer-motion": "^12.23.12",
    "geoip-lite": "^1.4.10",
    "isomorphic-dompurify": "^2.29.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.540.0",
    "motion": "^12.23.12",
    "nanoid": "^5.1.5",
    "next": "15.5.0",
    "next-themes": "^0.4.6",
    "prisma": "^6.14.0",
    "qr-code-styling": "^1.9.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-simple-maps": "^3.0.0",
    "recharts": "^2.15.4",
    "redis": "^5.8.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "ua-parser-js": "^2.0.4",
    "world-atlas": "^2.0.2",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^3.0.0",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-scale-chromatic": "^3.1.0",
    "@types/dompurify": "^3.2.0",
    "@types/geoip-lite": "^1.4.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-simple-maps": "^3.0.6",
    "@types/ua-parser-js": "^0.7.39",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5"
  }
}
